{
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": ["error", "windows"]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "ignorePatterns": ["webpack.config.js"]
}